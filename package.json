{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"description": "The smallest & fastest library for really easy, totally type-safe unit conversions in TypeScript & JavaScript",
	"devDependencies": {
		"@size-limit/preset-small-lib": "4.9.1",
		"@types/jest": "26.0.20",
		"@typescript-eslint/eslint-plugin": "4.13.0",
		"@typescript-eslint/parser": "4.13.0",
		"es-check": "5.2.0",
		"eslint-plugin-prettier": "3.3.1",
		"eslint-plugin-tree-shaking": "1.8.0",
		"eslint-plugin-tsdoc": "0.2.10",
		"prettier": "2.2.1",
		"prettier-config-xo": "1.0.3",
		"semantic-release": "17.3.3",
		"size-limit": "4.9.1",
		"ts-node": "9.1.1",
		"tsdx": "0.14.1",
		"tslib": "2.1.0",
		"typedoc": "0.20.15",
		"typescript": "4.1.3"
	},
	"files": [
		"dist"
	],
	"homepage": "https://convert.jonah.pw/",
	"keywords": [
		"convert",
		"convert-units",
		"unit-conversion",
		"uom",
		"metric",
		"imperial",
		"units",
		"unit",
		"measurement",
		"measure",
		"temperature",
		"mass",
		"weight",
		"length",
		"data"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/convert.esm.js",
	"name": "convert",
	"repository": {
		"type": "git",
		"url": "http://github.com/pizzafox/convert.git"
	},
	"scripts": {
		"analyze": "size-limit --why",
		"build": "tsdx build --format cjs,esm,umd",
		"compat": "es-check es3 dist/*.min.js",
		"docs": "typedoc",
		"lint": "tsdx lint",
		"size": "size-limit",
		"skip": "echo skip",
		"start": "tsdx watch --format cjs,esm,umd",
		"style": "prettier --check .",
		"test": "tsdx test"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"brotli": true,
			"limit": "3.5 KB",
			"path": "./dist/convert.cjs.production.min.js"
		},
		{
			"brotli": true,
			"limit": "3.6 KB",
			"path": "./dist/convert.umd.production.min.js"
		},
		{
			"gzip": true,
			"limit": "3.9 KB",
			"path": "./dist/convert.cjs.production.min.js"
		},
		{
			"gzip": true,
			"limit": "4 KB",
			"path": "./dist/convert.umd.production.min.js"
		}
	],
	"typings": "dist/index.d.ts",
	"version": "0.0.0-development"
}
