{
	"name": "convert",
	"version": "0.0.0-development",
	"description": "The smallest & fastest library for really easy, totally type-safe unit conversions in TypeScript & JavaScript",
	"keywords": [
		"convert",
		"convert-units",
		"unit-conversion",
		"uom",
		"metric",
		"imperial",
		"units",
		"unit",
		"measurement",
		"measure",
		"temperature",
		"mass",
		"weight",
		"length",
		"data"
	],
	"homepage": "https://convert.js.org/",
	"repository": {
		"type": "git",
		"url": "http://github.com/jonahsnider/convert.git"
	},
	"license": "MIT",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonah.pw",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "commonjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/convert.prod.mjs"
		},
		"./prod": {
			"require": "./dist/convert.prod.js",
			"import": "./dist/convert.prod.mjs"
		},
		"./dev": {
			"require": "./dist/convert.dev.js",
			"import": "./dist/convert.dev.mjs"
		}
	},
	"main": "./dist/index.js",
	"jsdelivr": "./dist/convert.prod.mjs",
	"unpkg": "./dist/convert.prod.mjs",
	"module": "./dist/convert.prod.mjs",
	"browser": "./dist/convert.prod.mjs",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"build": "rm -rf tsc_output dist; yarn run generate; tsc; rollup --config rollup.config.js; rm tsc_output/*.tsbuildinfo",
		"docs": "typedoc",
		"generate": "tsc --build ./dev.tsconfig.json; node tsc_output/dev/generate/index.js; tsc --build ./dev.tsconfig.json",
		"lint": "xo",
		"release": "semantic-release",
		"size": "size-limit",
		"style": "prettier --check .",
		"test": "yarn run generate; ava",
		"test:coverage": "yarn run generate; rm -rf coverage; c8 --reporter lcov --reporter text-summary --include 'tsc_output/**/*.js' --exclude 'tsc_output/{generated,test,dev,types}' --exclude 'tsc_output/*.test.js' --exclude 'tsc_output/entry.js' --all ava",
		"validate-api": "api-extractor run",
		"validate-api:local": "api-extractor run --local --verbose"
	},
	"prettier": "@jonahsnider/prettier-config",
	"ava": {
		"files": [
			"!src/test"
		],
		"require": [
			"./src/test/globals.js"
		],
		"typescript": {
			"rewritePaths": {
				"src/": "tsc_output/"
			},
			"compile": false
		}
	},
	"resolutions": {
		"terser": "5.10.0"
	},
	"devDependencies": {
		"@ava/typescript": "2.0.0",
		"@babel/core": "7.16.0",
		"@babel/preset-env": "7.16.4",
		"@jonahsnider/prettier-config": "1.1.0",
		"@jonahsnider/util": "8.4.3",
		"@jonahsnider/xo-config": "5.0.0",
		"@microsoft/api-extractor": "7.18.19",
		"@rollup/plugin-babel": "5.3.0",
		"@rollup/plugin-replace": "3.0.0",
		"@rollup/plugin-typescript": "8.3.0",
		"@size-limit/preset-small-lib": "^7.0.3",
		"@types/node": "16.11.10",
		"@types/prettier": "2.4.2",
		"ava": "3.15.0",
		"c8": "7.10.0",
		"decimal.js": "10.3.1",
		"prettier": "2.4.1",
		"rollup": "2.60.1",
		"rollup-plugin-terser": "7.0.2",
		"semantic-release": "18.0.0",
		"size-limit": "7.0.3",
		"terser": "5.10.0",
		"ts-node": "10.4.0",
		"tslib": "2.3.1",
		"typedoc": "0.22.9",
		"typescript": "4.4.4",
		"xo": "0.46.4"
	},
	"packageManager": "yarn@3.1.0",
	"runkitExampleFilename": "./docs/runkit.js",
	"size-limit": [
		{
			"brotli": true,
			"limit": "4.5 KiB",
			"path": "./dist/convert.prod.js"
		},
		{
			"gzip": true,
			"limit": "5.5 KiB",
			"path": "./dist/convert.prod.js"
		},
		{
			"brotli": true,
			"limit": "4.5 KiB",
			"path": "./dist/convert.prod.mjs"
		},
		{
			"gzip": true,
			"limit": "5.5 KiB",
			"path": "./dist/convert.prod.mjs"
		}
	]
}
