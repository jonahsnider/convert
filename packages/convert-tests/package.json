{
	"name": "convert-tests",
	"version": "0.0.0-development",
	"private": true,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"style": "prettier --check . --ignore-path ../../.prettierignore",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --100 --src '../convert/dist' --include '../convert/dist/convert.{dev,prod}.mjs' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.0",
		"@jonahsnider/util": "8.10.0",
		"@jonahsnider/xo-config": "5.0.3",
		"@tsconfig/node16": "1.0.2",
		"@types/node": "16.11.18",
		"ava": "4.0.0-rc.1",
		"bundled-conversions": "workspace:*",
		"c8": "7.11.0",
		"conversions": "workspace:*",
		"convert": "workspace:*",
		"optimized-conversions": "workspace:*",
		"prettier": "2.5.1",
		"ts-node": "10.4.0",
		"typescript": "4.5.4",
		"xo": "0.47.0"
	},
	"engines": {
		"node": "16.x"
	},
	"packageManager": "yarn@3.1.1"
}
