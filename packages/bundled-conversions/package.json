{
	"name": "bundled-conversions",
	"version": "0.0.0-development",
	"private": true,
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"exports": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"files": [
		"dist",
		"!dist/test"
	],
	"scripts": {
		"build": "node scripts/generate.mjs && tsc",
		"lint": "xo",
		"style": "prettier --check . --ignore-path ../../.prettierignore",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --100 --src 'src' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@tsconfig/node16": "1.0.3",
		"@types/node": "18.11.14",
		"ava": "5.1.0",
		"c8": "7.12.0",
		"codegen": "workspace:*",
		"conversions": "workspace:*",
		"optimized-conversions": "workspace:*",
		"prettier": "2.8.1",
		"ts-node": "10.9.1",
		"typescript": "4.9.4",
		"xo": "0.53.1"
	},
	"engines": {
		"node": "18.x"
	},
	"packageManager": "yarn@3.3.0"
}
