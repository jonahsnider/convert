{
	"name": "codegen",
	"version": "0.0.0-development",
	"private": true,
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"exports": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"files": [
		"dist",
		"!dist/test"
	],
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"style": "prettier --check . --ignore-path ../../.prettierignore",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --100 --src 'src' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"dependencies": {
		"@types/prettier": "2.6.0",
		"optimized-conversions": "workspace:*",
		"prettier": "2.6.2"
	},
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.1",
		"@jonahsnider/xo-config": "5.1.0",
		"@tsconfig/node16": "1.0.2",
		"@types/node": "16.11.39",
		"ava": "4.3.0",
		"c8": "7.11.3",
		"ts-node": "10.8.1",
		"typescript": "4.7.3",
		"xo": "0.50.0"
	},
	"engines": {
		"node": "16.x"
	},
	"packageManager": "yarn@3.2.1"
}
